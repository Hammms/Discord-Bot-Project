#packages used:
# discord.py | Beautiful Soup 4 | Google

import discord
import googlesearch

client = discord.Client()

@client.event
async def on_ready():
    print('We have logged in as {0.user}'.format(client))

@client.event
async def on_message(message):
    if message.author == client.user:
        return
    
    if message.content.startswith('jeff'):
      try: 
        from googlesearch import search 
      except ImportError:  
            print("No module named 'google' found") 

      query = message.content
      for j in search(query, tld="co.in", num=1, stop=1, pause=2): 
        await message.channel.send(j)
client.run('NzUzOTk4OTQyMDM0OTg1MDEx.X1uWPA.YyXYKlTAgQXHhD2Clwi8DkyY_Bg')

#on message for reciving information
#on ready to do something with that information 
#triggers on every message 