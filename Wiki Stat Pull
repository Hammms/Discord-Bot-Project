import requests
from bs4 import BeautifulSoup
#reads
res = requests.get('https://oldschool.runescape.wiki/w/Brine_sabre')
#Unpacks
sauce = BeautifulSoup(res.text, 'lxml')
print(sauce.select('.infobox-nested')[29])

#testing a for loop to get data into an array 
x = [5,6,7,8,9,15,16,17,18,19,25,26,27,28]
stats = []
for i in range(0,13):
    getstats = sauce.select('.infobox-nested')[x[i]]
    stats.insert(i,getstats)
    print(stats[i])


